<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>DishServlet</servlet-name>
    <servlet-class>refullapi.servlets.DishesServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>OrdersServlet</servlet-name>
    <servlet-class>refullapi.servlets.OrdersServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DishServlet</servlet-name>
    <url-pattern>/dishes/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OrdersServlet</servlet-name>
    <url-pattern>/orders/*</url-pattern>
  </servlet-mapping>
</web-app>
